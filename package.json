    "react-dom": "^16.4.1",
    "react-ionicons": "^2.1.6",
    "react-redux": "^5.0.7",
    "react-select": "^1.2.1",
    "redux": "^4.0.0",
    "redux-localstorage": "^0.4.1",
    "redux-thunk": "^2.3.0",
    "concurrently": "^3.6.0",
    "electron": "^2.0.4",
